{
    "name": "mohannadnaj/paraman",
    "description": "Laravel package for managing app Parameters/Settings.",
    "license": "MIT",
    "type": "package",
    "support": {
        "issues": "https://github.com/MohannadNaj/paraman/issues",
        "source": "https://github.com/MohannadNaj/paraman"
    },
    "homepage": "https://github.com/MohannadNaj/paraman",
    "authors": [
      {
        "name": "Mohannad Najjar",
        "email": "mohannad.najjar@mail.com"
      }
    ],
    "keywords": ["framework", "laravel"],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "~5.5",
        "illuminate/http": "~5.5",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9",
        "orchestra/database": "~3.5",
        "orchestra/testbench": "~3.5",
        "orchestra/testbench-browser-kit": "~3.5",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Paraman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paraman\\": "src/",
            "Paraman\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Paraman\\Providers\\ParametersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
